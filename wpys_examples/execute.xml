<?xml version="1.0" encoding="UTF-8"?>
<wps:Execute
	xmlns:wps="http://www.opengis.net/wps/2.0"
	xmlns:ows="http://www.opengis.net/ows/2.0"
	xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.opengis.net/wps/2.0 ../wps.xsd"
	
	service="WPS"
	version="2.0.0"
	response="document"
	mode="async">
	
	<ows:Identifier>long_running_process</ows:Identifier>
	<wps:Input id="sleep_time">5</wps:Input>	
	<!-- Query the default output format for the the result "BUFFERED_GEOMETRY" -->
	<!-- The result "BUFFERED_GEOMETRY" shall be delivered by reference in its default format -->
	<wps:Output id="distance" transmission="value"/>
	
</wps:Execute>